<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
		PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
		"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

	<classPathEntry location="H:\Ambient\Database\oracle\product\10.2.0\db_1\jdbc\lib\ojdbc14.jar" />

	<context id="mysqltables" targetRuntime="MyBatis3">

		<property name="autoDelimitKeywords" value="true" />
		<property name="beginningDelimiter" value="`" />
		<property name="endingDelimiter" value="`" />
		<commentGenerator>
			<property name="suppressDate" value="true" />
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<!--数据库链接地址账号密码 -->
		<jdbcConnection driverClass="oracle.jdbc.driver.OracleDriver"
						connectionURL="jdbc:oracle:thin:@localhost:1521:orcl" userId="ecology" password="weaver" />
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		<!--生成Model类存放位置 targetPackage 需要自定义 目录自动生成 basePackage -->
		<javaModelGenerator targetPackage="com.tingo.dto.origin" targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<!--生成映射文件存放位置 -->
		<sqlMapGenerator targetPackage="/mapper/origin" targetProject="src/main/resources">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>
		<!--生成Dao类存放位置 targetPackage 需要自定义 目录自动生成 basePackage-->
		<javaClientGenerator targetPackage="com.tingo.dao.origin" targetProject="src/main/java" type="XMLMAPPER">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>


		<!-- 正式生成之前需要删除 此事例 -->
